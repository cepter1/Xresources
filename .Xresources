!! URxvt Appearance
!URxvt.font: xft:Terminus:pixelsize=15:antialias=true
URxvt.font: xft:Inconsolata:pixelsize=14:antialias=true
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
!URxvt.internalBorder: 10
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 24
URxvt.iso14755: false
URxvt.iso14755_52: false

!! URxvt Functions
URxvt.perl-ext-common: font-size,selection-to-clipboard,pasta
URxvt.keysym.Shift-Control-C:    eval:selection_to_clipboard
URxvt.keysym.Shift-Control-V:    eval:paste_clipboard

URxvt.keysym.C-j:       perl:font-size:increase
URxvt.keysym.C-k:       perl:font-size:decrease

! For transparency

!URxvt*inheritPixmap: true
!URxvt*transparent: true
!URxvt*shading: 0 to 99 darkens, 101 to 200 lightens
!URxvt*shading: 50

! makes good grey background with current settings

!URxvt.depth: 32
!URxvt.background: rgba:1111/1111/1111/dddd

